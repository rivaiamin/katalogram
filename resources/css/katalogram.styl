.kg-crop-area {
    background: #E4E4E4;
    overflow: hidden;
    width:500px;
    height:350px;
}

.flip-clock-divider .flip-clock-label { color: #fff; }
.tagsinput > div > input { border:none !important; width: 100%; }
tags-input .tags .tag-item { background: bg-base2; color: #fff; border-color:bg-base; }
tags-input .tags .tag-item.selected { background: bg-base; color: #fff; }
tags-input .tags { border-radius: 3px; border: none; }
tags-input .autocomplete .suggestion-item.selected, tags-input .autocomplete .suggestion-item.selected em { background: bg-base2; }
#kgHero { z-index:2; height:300px; }
#kgHero .content { z-index:2; }
#kgHero:not(.sticky) { margin-top: -80px!important; position: relative; }
#kgHero.sticky { margin-top:-240px; }

@media (max-width: 1100px) {
    .modal { width: 100%; left: 0; }
    .modal-footer { width: 100%; }
}

::-webkit-scrollbar {
    width: 12px;
    position: relative;
    z-index:1050;
}
::-webkit-scrollbar-track {
    background: #eaeaea;
    border-left: 1px solid #ccc;
}
::-webkit-scrollbar-thumb {
    background: bg-base;
}
::-webkit-scrollbar-thumb:hover {
    background: bg-base2;
}

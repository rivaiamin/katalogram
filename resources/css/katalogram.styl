.ui.comments.right .comment .avatar { float: right; }
.ui.comments.right .comment .content { margin-right: 3.5em; margin-left: 0; }
.ui.labeled.icon.menu .item { min-width: auto; }
.ui.card.uk-container-center { margin: 0.5em auto; }
.a-inverted * { color:#333 !important; }
.ui.four.cards>.card { width: calc(25% - 1em); margin-left:0.5em; margin-right:0.5em; }
.ui.link.cards .card:hover { z-index: 1 }
.item.search { width:650px; }
.item.search tags-input { width:100%; }
.ui.menu .item.head>img:not(.ui) { width: 3em; }
.ui.corner.label, .ui.corner.label:hover { font-size: 1.3em; }
.ui.primary.label { background: color-base; color: #fff; }
.ui.secondary.label, .ui.menu .item>.secondary.label { background: color-base2; color: #fff; }
.ui.secondary.corner.label { border-color: color-base2; }
.ui.primary.corner.label { border-color: color-base; }
.ui.left.corner.label .icon { left: -0.5em }
.ui.corner.label .icon { font-size:1.5em; top:0.3em;  }
